	<queryString><![CDATA[select sum((rt.estimated_time/8)* r.cost_per_hour) as est_hor_proj, 
       sum((rt.actual_time/8)* r.cost_per_hour) as AC,
1000 - sum((rt.actual_time/8)* r.cost_per_hour) as EV
from resource_task rt, resource r
where rt.resource_id = r.id
and rt.project_id = r.project_id
and r.cost_per_hour is not null
]]></queryString>

