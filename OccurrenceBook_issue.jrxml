	<queryString><![CDATA[select o.id, o.project_id, o.occurrence_status_label, 
       p.creation_date, o.name, sub.* 
from occurrence o, planning p, ( 

select ac.id, ac.value as ac, an.value as an, fo.value as fo, im.value as im, re.value as re from (
	select occurrence_id as id, value from occurrence_field where field='ACTION') as ac, (
	select occurrence_id as id, value from occurrence_field where field='ANALYSIS') as an, (
	select occurrence_id as id, value from occurrence_field where field='FORECAST') as fo, (
	select occurrence_id as id, value from occurrence_field where field='IMPACT') as im, (
	select occurrence_id as id, value from occurrence_field where field='RESPONSIBL') as re
where ac.id = an.id 
  and an.id = fo.id 
  and fo.id = im.id 
  and im.id = re.id 
) as sub
where o.id = p.id 
  and sub.id = o.id
  and o.source='com.pandora.bus.occurrence.IssueOccurrence'
  and sub.id = $P{OCCURRENCE_ID}
order by p.creation_date]]></queryString>

