	<queryString><![CDATA[select o.id, o.project_id, o.occurrence_status_label, 
       p.creation_date, o.name, sub.* 
from occurrence o, planning p, ( 

select dt.id, dt.value as dt, de.value as de, vi.value as vi from (
	select occurrence_id as id, value from occurrence_field where field='DATE') as dt, (
	select occurrence_id as id, value from occurrence_field where field='DESCRIPTIO') as de, (
	select occurrence_id as id, value from occurrence_field where field='VISIBILITY') as vi
where dt.id = de.id 
  and de.id = vi.id 
) as sub
where o.id = p.id 
  and sub.id = o.id
  and o.source='com.pandora.bus.occurrence.MilestoneOccurrence'
  and sub.id = $P{OCCURRENCE_ID}
order by p.creation_date]]></queryString>

