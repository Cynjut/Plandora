		<defaultValueExpression ><![CDATA["229"]]></defaultValueExpression>
	</parameter>
	<queryString><![CDATA[select o.project_id, o.occurrence_status_label, 
       p.creation_date, o.name, sub.* 
from occurrence o, planning p, ( 

select co.id, co.value as co, dt.value as dt, de.value as de, du.value as du, lo.value as lo, re.value as re, ti.value as ti, vi.value as vi from (
	select occurrence_id as id, value from occurrence_field where field='CONCLUSION') as co, (
	select occurrence_id as id, value from occurrence_field where field='DATE') as dt, (
	select occurrence_id as id, value from occurrence_field where field='DESCRIPTIO') as de, (
	select occurrence_id as id, value from occurrence_field where field='DURATION') as du, (
	select occurrence_id as id, value from occurrence_field where field='LOCATION') as lo, (
	select occurrence_id as id, value from occurrence_field where field='REASON') as re, (
	select occurrence_id as id, value from occurrence_field where field='TIME') as ti, (
	select occurrence_id as id, value from occurrence_field where field='VISIBILITY') as vi
where co.id = dt.id 
  and dt.id = de.id 
  and de.id = du.id 
  and du.id = lo.id 
  and lo.id = re.id 
  and re.id = ti.id 
  and ti.id = vi.id) as sub
where o.id = p.id 
  and sub.id = o.id
  and o.source='com.pandora.bus.occurrence.EventOccurrence'
  and sub.id = $P{OCCURRENCE_ID}
order by p.creation_date]]></queryString>

