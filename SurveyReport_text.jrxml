<?xml version="1.0" encoding="UTF-8"?>
<jasperReport xmlns="http://jasperreports.sourceforge.net/jasperreports" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://jasperreports.sourceforge.net/jasperreports http://jasperreports.sourceforge.net/xsd/jasperreport.xsd" name="SurveyReport_text" pageWidth="555" pageHeight="802" columnWidth="555" leftMargin="0" rightMargin="0" topMargin="0" bottomMargin="0">
	<property name="ireport.zoom" value="1.0"/>
	<property name="ireport.x" value="0"/>
	<property name="ireport.y" value="0"/>
	<parameter name="SURVEY_ID" class="java.lang.String">
		<parameterDescription><![CDATA[]]></parameterDescription>
	</parameter>
	<parameter name="QUESTION_ID" class="java.lang.String"/>
	<queryString>
		<![CDATA[select a.survey_id, a.question_id, a.user_id, a.value, a.answer_date, t.username
from survey_question q, question_answer a LEFT OUTER JOIN tool_user t on t.id = a.user_id
where a.question_id = $P{QUESTION_ID}
  and a.survey_id = $P{SURVEY_ID}
  and (q.type = '1' or q.type = '2')
  and a.survey_id = q.survey_id
  and a.question_id = q.id
order by a.answer_date]]>
	</queryString>
	<field name="survey_id" class="java.lang.String">
