	</parameter>
	<queryString><![CDATA[select o.id, o.project_id, o.occurrence_status_label, 
       p.creation_date, o.name, sub.* 
from occurrence o, planning p, ( 

select de.id, idt.value as idt, fdt.value as fdt, de.value as de from (
	select occurrence_id as id, value from occurrence_field where field='INI_DATE') as idt, (
	select occurrence_id as id, value from occurrence_field where field='FINAL_DATE') as fdt, (
	select occurrence_id as id, value from occurrence_field where field='DESCRIPTIO') as de
where idt.id = fdt.id 
  and fdt.id = de.id 
) as sub
where o.id = p.id 
  and sub.id = o.id
  and o.source='com.pandora.bus.occurrence.IterationOccurrence'
  and sub.id = $P{OCCURRENCE_ID}
order by p.creation_date]]></queryString>

